{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "defaultDestination": "wwwroot/lib/",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.2.1",
      "files": [
        "css/bootstrap.css",
        "js/bootstrap.js",
        "css/bootstrap.min.css",
        "js/bootstrap.min.js"
      ],
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "jquery@3.3.1",
      "files": [ "jquery.slim.js", "jquery.slim.min.js" ],
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "popper.js@1.14.6",
      "files": [ "popper.js", "popper.min.js" ],
      "destination": "wwwroot/lib/popper"
    }
  ]
}
